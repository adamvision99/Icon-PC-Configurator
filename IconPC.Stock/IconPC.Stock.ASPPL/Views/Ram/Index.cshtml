@model IEnumerable<IconPC.Stock.ASPPL.Models.RAM>

@{
    ViewBag.Title = "Index";
}

<h2>Список</h2>

<p>
    @Html.ActionLink("Создать", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Size)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NumberOfModules)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MemoryFrequency)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Latency)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Size)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NumberOfModules)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MemoryFrequency)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Latency)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
